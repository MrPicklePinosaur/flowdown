
WHITESPACE = _{ " " }

lines = {
    ((command_block|utterance_block)? ~ NEWLINE)*
}

command_block = {
    "[" ~ command_body ~ "]"
}
command_body = {
    end_command_body |  code_command_body
}
end_command_body = {
    "end"
}
code_command_body = @{
    "code" ~ WHITESPACE+ ~ js_file_name
}

utterance_block = {
    ASCII_ALPHANUMERIC+
}

js_file_name = @{
    ASCII_ALPHANUMERIC+ ~ ".js"    
}
